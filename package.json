{
  "name": "sdcs-slack-bot",
  "version": "0.0.1",
  "main": "server/server.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "parcel build client/src/html/index.html client/src/html/login.html && parcel build client/src/js/components/StudentView/StudentDashboard.js",
    "dev": "parcel watch client/src/html/index.html client/src/html/login.html",
    "lint": "eslint .",
    "start": "node ./server/server.js",
    "cypress": "./node_modules/.bin/cypress open",
    "test": "jest --no-watchman --verbose --coverage; mocha test/server/*.spec.js --exit",
    "coverage": "nyc npm run test"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./test/client/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "/test/server/"
    ]
  },
